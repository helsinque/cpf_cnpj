{
    "name": "helsinque/cpf-cnpj",
    "description": "Validador para CPF e CNPJ agnÃ³stico de framework",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "hel-sys",
            "email": "helsinquedeveloper@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.5.9",
        "bipbop/api": "*"
      
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2",
        "phpunit/php-code-coverage": "^3",
        "squizlabs/php_codesniffer": "2.*",
        "brainmaestro/composer-git-hooks": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Helsinque\\": "src/",
            "Validators\\": "src/Validators",
            "Exceptions\\": "src/Exceptions"
        }
    },
    "hooks": {
        "pre-commit": "./vendor/bin/phpcs --standard=PSR2 src/",
        "pre-push": "./vendor/bin/phpunit"
    },
    "scripts": {
        "cghooks": "vendor/bin/cghooks",
        "post-install-cmd": "vendor/bin/cghooks add --ignore-lock",
        "post-update-cmd": "vendor/bin/cghooks update"
    }
}
